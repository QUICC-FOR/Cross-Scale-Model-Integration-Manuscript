	Doi = {10.1111/j.1365-2699.2009.02227.x},
	Isbn = {0305-0270},
	Journal = {Journal of Biogeography},
	Mendeley-Groups = {METHODS/MODELS/SDMs/Hybrid-SDMs,\_PROJECTS/\_Projet CrossScaleModel},
